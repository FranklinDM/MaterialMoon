<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <!-- General -->
        <em:id>materialmoon@franklindm</em:id>
        <em:version>dev-build</em:version>
        <em:type>4</em:type>
        <em:internalName>MMChrome</em:internalName>
        <em:skinnable>true</em:skinnable>

        <!-- Front-end metadata -->
        <em:name>Material Moon</em:name>
        <em:description>Material Chrome-like theme for Pale Moon. Fork of PMChrome by Lootyhoof.</em:description>
        <em:homepageURL>https://franklindm.github.io/</em:homepageURL>

        <!-- Authors -->
        <em:creator>FranklinDM</em:creator>
        <em:contributor>Lootyhoof</em:contributor>
        <em:contributor>dkgo</em:contributor>
        <em:contributor>Chrome</em:contributor>
        <em:contributor>Google Material Icons</em:contributor>
        <em:contributor>Graciliano (Win10 caption buttons)</em:contributor>

        <!-- Targeted applications -->
        <em:targetApplication>
            <!-- Pale Moon -->
            <Description>
                <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
                <em:minVersion>28.0.0a1</em:minVersion>
                <em:maxVersion>29.*</em:maxVersion>
            </Description>
        </em:targetApplication>
        <em:targetApplication>
            <!-- Applications using Firefox GUID -->
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <!--
                    Pale Moon 30 and above will stop using its own GUID to
                    "further improve extension compatibility"
                 -->
                <em:minVersion>30.0.0a1</em:minVersion>
                <em:maxVersion>30.*</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>
